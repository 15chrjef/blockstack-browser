<Target Name="DevEnvInstaller">
<PropertyGroup>
<DevEnv>C:\Program Files (x86)\Microsoft Visual Studio 10.0\Common7\IDE\devenv.com</DevEnv>
 
<ProjectFile>$(SolutionRoot)\Setup1\Setup1.vdproj</ProjectFile>
<Configuration>Release|Any Cpu</Configuration>
</PropertyGroup>
 <Exec>
 Command="&quot;$(devenv)&quot; /Project &quot;$(ProjectFile)&quot; /Rebuild  &quot;$(Configuration)&quot; /Log"
 ContinueOnError="false"
 IgnoreExitCode="false"
 WorkingDirectory="$(MSBuildProjectDirectory)" />
</Target>