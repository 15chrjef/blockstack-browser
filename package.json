    "blockstack-profiles": "^0.3.2",
    "bootstrap": "git+ssh://git@github.com/blockstack/blockstack-bootstrap.git",
    "electron-debug": "^0.5.1",
    "font-awesome": "^4.4.0",
    "hasprop": "0.0.4",
    "history": "^2.0.0-rc2",
    "is-retina": "^1.0.3",
    "knox": "^0.9.2",
    "react": "^0.14.6",
    "react-bootstrap": "^0.28.2",
    "react-dom": "^0.14.6",
    "react-mixin": "^3.0.3",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^2.1.0",
    "react-select": "^0.9.1",
    "redux": "^3.0.5",
    "redux-localstorage": "^0.4.0",
    "redux-promise": "^0.5.1",
    "redux-thunk": "^1.0.3",
    "triplesec": "^3.0.25"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  }
}
